{
  "title": "API – Bike Predictions KPIs",
  "uid": "bike-api-kpis",
  "timezone": "browser",
  "schemaVersion": 39,
  "refresh": "30s",
  "tags": [
    "mlops",
    "bike",
    "api"
  ],
  "time": {
    "from": "now-24h",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "job",
        "label": "Prometheus job",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "query": "label_values(up, job)",
        "regex": "/.*api.*/",
        "refresh": 1,
        "sort": 1,
        "multi": false,
        "includeAll": false
      },
      {
        "name": "handler",
        "label": "Endpoint",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "query": "label_values(http_requests_total{job=\"$job\"}, handler)",
        "refresh": 1,
        "sort": 1,
        "multi": true,
        "includeAll": true
      }
    ]
  },
  "panels": [
    {
      "type": "row",
      "title": "Synthèse (dernière heure)",
      "collapsed": false,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "id": 100
    },
    {
      "type": "stat",
      "title": "Evaluation du Rate interval (s)",
      "id": 99,
      "gridPos": { "h": 6, "w": 4, "x": 0, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "refId": "A",
          "expr": "clamp_min(vector($__interval + 5), 20)",
          "instant": true
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s", "decimals": 0 } },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },

    {
      "type": "stat",
      "title": "Disponibilité – 1h",
      "id": 1,
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 4,
        "y": 1
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "avg_over_time(up{job=\"$job\"}[1h]) * 100"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red"
              },
              {
                "color": "yellow",
                "value": 95
              },
              {
                "color": "green",
                "value": 99
              }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      }
    },
    {
      "type": "stat",
      "title": "Req/s – (sur rate_interval_s)",
      "id": 2,
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 10,
        "y": 1
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(http_requests_total{job=\"$job\"}[$__rate_interval]))"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        }
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      }
    },
    {
      "type": "stat",
      "title": "Taux d'erreur (5xx) – (sur rate_interval_s)",
      "id": 3,
      "gridPos": {
        "h": 6,
        "w": 4,
        "x": 16,
        "y": 1
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "100 * sum(rate(http_requests_total{job=\"$job\",status=~\"5..\"}[$__rate_interval])) / sum(rate(http_requests_total{job=\"$job\"}[$__rate_interval]))"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green"
              },
              {
                "color": "yellow",
                "value": 1
              },
              {
                "color": "red",
                "value": 5
              }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      }
    },
    {
      "type": "stat",
      "title": "Préd/req – moy (sur rate_interval_s)",
      "id": 4,
      "gridPos": {
        "h": 6,
        "w": 4,
        "x": 20,
        "y": 1
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(api_predictions_per_response_sum{job=\"$job\"}[$__rate_interval])) / sum(rate(api_predictions_per_response_count{job=\"$job\"}[$__rate_interval]))"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none"
        }
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      }
    },
    {
      "type": "row",
      "title": "Latence (quantiles sur rate_interval_s)",
      "collapsed": false,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 7
      },
      "id": 200
    },
    {
      "type": "stat",
      "title": "p50 latence – (sur rate_interval_s)",
      "id": 10,
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 0,
        "y": 8
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.5, sum(rate(http_request_duration_seconds_bucket{job=\"$job\"}[$__rate_interval])) by (le))"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      }
    },
    {
      "type": "stat",
      "title": "p90 latence – (sur rate_interval_s)",
      "id": 11,
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 8,
        "y": 8
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.9, sum(rate(http_request_duration_seconds_bucket{job=\"$job\"}[$__rate_interval])) by (le))"
