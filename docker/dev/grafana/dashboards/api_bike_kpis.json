      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(http_requests_total{job=\"$job\", handler=~\".*predictions.*\"}[$__rate_interval]))"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        }
      },
      "options": {
        "legend": {
          "show": false
        },
        "tooltip": {
          "mode": "single"
        }
      }
    },
    {
      "type": "bargauge",
      "title": "Top 5 endpoints (Req/s)",
      "id": 22,
      "gridPos": {
        "h": 6,
        "w": 24,
        "x": 0,
        "y": 23
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(5, sum by (handler) (rate(http_requests_total{job=\"$job\"}[$__rate_interval])))"
        }
      ],
      "options": {
        "displayMode": "gradient"
      }
    },
    {
