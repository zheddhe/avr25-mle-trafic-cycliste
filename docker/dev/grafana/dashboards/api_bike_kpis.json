      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      }
    },
    {
      "type": "row",
      "title": "Débit & erreurs (prédictions uniquement)",
      "collapsed": false,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 14
      },
      "id": 300
    },
    {
      "type": "timeseries",
      "title": "Requêtes par code – (/predictions/) (sur rate_interval_s)",
      "id": 20,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 15
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (status) (rate(http_requests_total{job=\"$job\", handler=~\".*predictions.*\"}[$__rate_interval]))"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right"
        },
        "tooltip": {
          "mode": "single"
        },
        "stacking": {
          "mode": "normal"
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Req/s total – (/predictions/) (sur rate_interval_s)",
      "id": 21,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 15
      },
      "datasource": {
        "type": "prometheus",
