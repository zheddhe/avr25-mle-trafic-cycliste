        }
      }
    },
    {
      "type": "stat",
      "title": "p99 latence – (sur rate_interval_s)",
      "id": 12,
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 16,
        "y": 8
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket{job=\"$job\"}[$__rate_interval])) by (le))"
