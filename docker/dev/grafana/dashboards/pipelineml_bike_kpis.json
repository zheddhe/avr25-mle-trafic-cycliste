      "id": 3,
      "gridPos": { "h": 6, "w": 8, "x": 16, "y": 7 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "refId": "A",
          "expr": "last_over_time(bike_r2{job=\"bike-traffic\",site=\"$site\"}[6h])"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red" },
              { "color": "yellow", "value": 0.5 },
              { "color": "green", "value": 0.8 }
            ]
          }
        }
      },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },

    { "type": "row", "title": "Historique 24h (test)", "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 14 }, "id": 300
    },

    {
      "type": "timeseries", "title": "MAPE – 24h", "id": 801,
      "gridPos": { "h": 9, "w": 8, "x": 0, "y": 15 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [ { "refId": "A",
        "expr": "avg_over_time(bike_mape{job=\"bike-traffic\",site=\"$site\"}[1h])"
      } ],
      "fieldConfig": { "defaults": {
        "unit": "percent",
        "color": { "mode": "thresholds" },
        "thresholds": { "mode": "absolute",
          "steps": [
            { "color": "green" },
            { "color": "yellow", "value": 30 },
            { "color": "red", "value": 60 }
          ]
        },
        "custom": {
          "drawStyle": "line",
          "lineInterpolation": "smooth",
          "lineWidth": 2,
          "fillOpacity": 20,
          "spanNulls": true,
          "gradientMode": "opacity",
          "showPoints": "never"
        }
      } },
      "options": { "legend": { "show": false }, "tooltip": { "mode": "single" } }
    },

    {
