{
  "title": "Pipeline ML – Business KPIs",
  "uid": "bike-pipelineml-kpis",
  "timezone": "browser",
  "schemaVersion": 39,
  "refresh": "5s",
  "tags": ["mlops", "bike", "pipelineML"],
  "templating": {
    "list": [
      {
        "name": "site",
        "label": "Counter ID",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "query": "label_values(bike_mape, site)",
        "refresh": 1,
        "sort": 1,
        "multi": false,
        "includeAll": false
      }
    ]
  },
  "panels": [
    { "type": "row", "title": "Informations générales (dernière exécution)", "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 }, "id": 100
    },

    {
      "type": "stat",
      "title": "Observations réelles",
      "id": 4,
      "gridPos": { "h": 6, "w": 6, "x": 0, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "refId": "A",
          "expr": "last_over_time(bike_obs_count{job=\"bike-traffic\",site=\"$site\",kind=\"true\"}[6h])"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "color": { "mode": "fixed", "fixedColor": "text" }
        }
      },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "stat",
      "title": "Observations prédites",
      "id": 5,
      "gridPos": { "h": 6, "w": 6, "x": 6, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "refId": "A",
          "expr": "last_over_time(bike_obs_count{job=\"bike-traffic\",site=\"$site\",kind=\"pred\"}[6h])"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "color": { "mode": "fixed", "fixedColor": "text" }
        }
      },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "stat",
      "title": "Décalage simulé (dayX, jours)",
      "id": 6,
      "gridPos": { "h": 6, "w": 6, "x": 12, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "refId": "A",
          "expr": "last_over_time(bike_data_day_offset_days{job=\"bike-traffic\",site=\"$site\"}[6h])"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "d",
          "color": { "mode": "fixed", "fixedColor": "text" }
        }
      },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "stat",
      "title": "Fraîcheur réelle (jours) – last",
      "id": 7,
      "gridPos": { "h": 6, "w": 6, "x": 18, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "refId": "A",
          "expr": "last_over_time(bike_data_freshness_days{job=\"bike-traffic\",site=\"$site\"}[6h])"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "d" } },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },

    { "type": "row", "title": "Métriques (dernière exécution)", "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 7 }, "id": 200
    },

    {
      "type": "stat",
      "title": "MAPE (%) – test – last",
      "id": 1,
      "gridPos": { "h": 6, "w": 8, "x": 0, "y": 7 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "refId": "A",
          "expr": "last_over_time(bike_mape{job=\"bike-traffic\",site=\"$site\"}[6h])"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green" },
              { "color": "yellow", "value": 30 },
              { "color": "red", "value": 60 }
            ]
          }
        }
      },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "stat",
      "title": "RMSE – test – last",
      "id": 2,
      "gridPos": { "h": 6, "w": 8, "x": 8, "y": 7 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "refId": "A",
          "expr": "last_over_time(bike_rmse{job=\"bike-traffic\",site=\"$site\"}[6h])"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green" },
              { "color": "yellow", "value": 50 },
              { "color": "red", "value": 100 }
            ]
          }
        }
      },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "stat",
